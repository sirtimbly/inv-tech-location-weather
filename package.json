{
  "name": "inv-tech-location-weather",
  "version": "1.0.0",
  "description": "converts locations to weather info from command line",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirtimbly/inv-tech-location-weather.git"
  },
  "author": "Tim Bendt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sirtimbly/inv-tech-location-weather/issues"
  },
  "homepage": "https://github.com/sirtimbly/inv-tech-location-weather#readme",
  "devDependencies": {
    "@types/node": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-typescript": "^0.8.0",
    "typescript": "^3.3.3333",
    "xo": "^0.24.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "xo": {
    "globals": [
      "window",
      "document"
    ],
    "rules": {
      "import/no-unresolved": false
    },
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "ignore": [
      "src/patterns/templates",
      "fractal.js"
    ]
  },
  "dependencies": {
    "ts-node": "^8.0.3"
  }
}
