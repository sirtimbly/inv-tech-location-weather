{
  "name": "inv-tech-location-weather",
  "version": "1.0.0",
  "description": "converts locations to weather info from command line",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node --nolazy -r ts-node/register --inspect=9229 src/index.ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirtimbly/inv-tech-location-weather.git"
  },
  "author": "Tim Bendt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sirtimbly/inv-tech-location-weather/issues"
  },
  "homepage": "https://github.com/sirtimbly/inv-tech-location-weather#readme",
  "devDependencies": {
    "@types/node": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-typescript": "^0.8.0",
    "typescript": "^3.3.3333",
    "xo": "^0.24.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "xo": {
    "semicolon": false,
    "singleQuote": false,
    "rules": {
      "no-empty-pattern": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/no-require-imports": "off"
    },
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ]
  },
  "dependencies": {
    "@types/date-fns": "^2.6.0",
    "axios": "^0.21.1",
    "date-fns": "^1.30.1",
    "emojiweather": "^0.1.0",
    "ts-node": "^8.0.3"
  }
}
